	Pages = {{69-81}},
	Title = {{A Monte Carlo Markov chain algorithm for a class of mixture time series models}},
	Unique-Id = {{ISI:000285363700006}},
	Volume = {{21}},
	Year = {{2011}},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s11222-009-9147-6%7D}}

@article{Lau:2008xw,
	Abstract = {{An infinite mixture of autoregressive models is developed. The unknown
   parameters in the mixture autoregressive model follow a mixture
   distribution, which is governed by a Dirichlet process prior. One main
   feature of our approach is the generalization of a finite mixture model
   by having the number of components unspecified. A Bayesian sampling
   scheme based on a weighted Chinese restaurant process is proposed to
   generate partitions of observations. Using the partitions, Bayesian
   prediction, while accounting for possible model uncertainty,
   determining the most probable number of mixture components, clustering
   of time series and outlier detection in time series, can be done.
   Numerical results from simulated and real data are presented to
   illustrate the methodology. (c) 2008 Elsevier B.V. All rights reserved.}},
	Author = {Lau, John W. and So, Mike K. P.},
	Date-Added = {2011-07-14 16:01:39 -0700},
