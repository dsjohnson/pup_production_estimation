	Journal = {Journal of Applied Statistics},
	Keywords = {annual survival,capture-recapture,mark-,mark-recapture,movement,of-fit tests,populations,probabilities,recapture,recruitment,roseate terns,tag-recovery data},
	Mendeley-Tags = {mark-recapture},
